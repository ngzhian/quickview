{
    "manifest_version": 2,
    "version": "2.0.2",
    "name": "Quickview for YouTube",
    "description": "Allows you to quickly view videos from your subscriptions feed page",
    "icons": { "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png" },
    "content_scripts": [
    {
        "matches": ["*://www.youtube.com/*"],
        "css": ["qv.css"],
        "js": ["lib/jquery-1.9.1.min.js", "lib/hoverIntent.min.js", "lib/jquery-ui-1.10.2.custom.js", "qv.js"],
        "run_at": "document_end"
    }
    ]
}
